{
  "extends": [
    "airbnb"
  ],
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "import/no-named-as-default": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "semi": 0
  }
}